<!doctype html>
<html lang="en">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport"
        content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, viewport-fit=cover" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <meta name="theme-color" content="#000000">
    <title>Admin Dashboard - <%=user.fullname%></title>
    <meta name="description" content="zoe1">
    <meta name="keywords"
        content="bootstrap, wallet, banking, fintech mobile template, cordova, phonegap, mobile, html, responsive" />
    <link rel="icon" type="image/png" href="/assets/img/favicon.png" sizes="32x32">
    <link rel="apple-touch-icon" sizes="180x180" href="assets/img/icon/192x192.png">
    <link rel="stylesheet" href="/assets/css/style.css">
    <link rel="manifest" href="__manifest.json">
    <script src="../plugins/sweetalerts/promise-polyfill.js"></script>
    <link href="../plugins/sweetalerts/sweetalert.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
</head>

<body style="background-color:#ffffff;">

    <!-- loader 
    <div id="loader">
        <img src="assets/img/loading-icon.png" alt="icon" class="loading-icon">
    </div>-->
    <!-- * loader -->

    <!-- App Header -->
    <div class="appHeader bg-primary text-light">
        <div class="left">
            <a href="#" class="headerButton" data-bs-toggle="modal" data-bs-target="#sidebarPanel">
                <ion-icon name="menu-outline"></ion-icon>
            </a>
        </div>
        <div class="pageTitle">
            <p><%=user.fullname%></p>
        </div>
        <div class="right">
            <a href="/profile" class="headerButton">
                <img src="/assets/img/sample/avatar/avatar1.jpg" alt="image" class="imaged w32">
            </a>
        </div>
    </div>
    <!-- * App Header -->

<div>
    <!-- App Capsule -->
    <div id="appCapsule">
      <div class="d-flex justify-content-between flex-wrap flex-md nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2 text-gray">Editing:  <%=user.fullname%> </h1>
        <div class="btn-toolbar mb-2 mb-md 0">
        </div>
      </div>
      
      <div class="col py-3">
        <div class="row">
          <div class="col">
            <nav aria-label="breadcrumb">
              <ol class="breadcrumb">
                <li class="breadcrumb-item active"> <%=user.fullname%> </li>
              </ol>
            </nav>
          </div>
          <div class="col text-end fw-lighter">
            <!-- <b>Last Updated:</b> -->
            <b>UserId:</b> <%=user._id%>
          </div>
        </div>
      
      </div>
      
      
      <form action="/editUser/<%=user._id%>?_method=PUT" method="POST">
      
        <div class="row form-group mb-4">
          <div class="col">
            <label for="fullname">Full Name</label>
            <input type="text" class="form-control" id="fullname" name="fullname" value=" <%=user.fullname%> " placeholder="Full Name" readonly required>
          </div>
    
          
          <div class="col">
            <label for="text">Country</label>
            <input type="text" class="form-control" id="country" name="country" value=" <%=user.country%> " placeholder="country" readonly required>
          </div>
          <div class="row form-group mb-4">
          <!-- <div class="col">
            <label for="text">Gender</label>
            <input type="text" class="form-control" id="gender" name="gender" value=" user.gender%> " placeholder="Gender" readonly required>
          </div>
           -->
          <div class="col">
            <label for="balance">Deposit</label>
            <input type="text" class="form-control" id="balance" name="balance" value=" <%=user.balance%> " placeholder="balance" required>
          </div>
    
        </div>
    
        </div>
    
        <div class="row form-group mb-4">
          <div class="col">
            <label for="withdrawBalance">Widthdraw </label>
            <input type="text" class="form-control" id="pending" name="pending" value=" <%=user.pending%> " placeholder="balance" required>
          </div>
    
          <div class="col">
            <label for="bonus">Bonus </label>
            <input type="text" class="form-control" id="bonus" name="bonus" value=" <%=user.balance%>" placeholder="balance" required>
          </div>
    
        </div>
          
        <div class="row form-group mb-4">
          <div class="col">
            <label for="tel">Telephone</label>
            <input type="tel" class="form-control" id="tel" name="tel" value=" <%=user.tel%> " placeholder="Telephone" readonly required>
          </div>
      
          <div class="col">
            <label for="email">Email</label>
            <input type="email" class="form-control" id="email" name="email" value="<%=user.email%>" placeholder="Email" readonly required>
          </div>
        </div>
    
        <div class="row form-group mb-4">
    
        <div class="col">
          <label for="available">Last Deposit</label>
          <input type="text" class="form-control" id="lastDeposit" name="lastDeposit" value=" <%=user.lastDeposit%> " placeholder="last Deposit" required>
        </div>
    
        <!-- <div class="col">
          <label for="session">Trade sessions </label>
          <input type="text" class="form-control" id="session" name="session" value=" <%=user.session%> " placeholder="0/0" required>
        </div> -->
    
      </div>
    
      <div class="row form-group mb-4">
        <div class="col">
          <label for="profit">Profit</label>
          <input type="text" class="form-control" id="profit" name="profit" value=" <%=user.profit%> " placeholder="Profile" required>
        </div>
    
        <!-- <div class="col">
          <label for="totalDeposit">Total Deposit</label>
          <input type="text" class="form-control" id="totalDeposit" name="totalDeposit" value=" user.totalDeposit%> " placeholder="Total Deposit" required>
        </div> -->
      </div>
    
      <div class="row form-group mb-4">
        <div class="col">
          <label for="verifiedStatus">Verification status</label>
          <input type="text" class="form-control" id="verifiedStatus" name="verifiedStatus" value=" <%=user.verifiedStatus%> " placeholder="Verification status" required>
        </div>
      </div>
    
      <div class="row form-group mb-4">
        <!-- <div class="col">
          <label for="totalWidthdraw">Total Widthdraw</label>
          <input type="text" class="form-control" id="totalWidthdraw" name="totalWidthdraw" value="user.totalWidthdraw%>" placeholder="Total widthdraw" required>
        </div> -->
    
        <div class="col">
          <label for="account">Account Level</label>
          <input type="text" class="form-control" id="account" name="account" value=" <%=user.account%> " placeholder="Account level" required>
        </div>
      </div>
      
      
        <div class="form-group mb-4">
          <button type="submit" class="btn btn-primary">Update User</button>
      
          <button type="button" class="btn btn-danger" data-bs-toggle="modal" id="deleteButton" data-bs-target="#deleteModal">Delete User</button>
      
        </div>
      
      </form>
      
      <div class="modal fade" tabindex="-1" role="dialog" id="deleteModal">
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <div class="modal-title">You are about to remove a user record.</div>
              <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
              <p>
                This will remove the User record of <b class="fw-bold"> <%=user.fullname%> </b><br/>
                Are you sure?
              </p>
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
              <form action="/deleteUser/<%=user._id%> ?_method=DELETE" method="POST" class="position-relative">
                <button type="submit" class="btn btn-primary">Yes, Remove User</button>
              </form>
            </div>
          </div>
        </div>
      </div>
    

        <!-- app footer -->
        <div class="appFooter">
            <div class="footer-title">
                Copyright Â© Trade Genesis. All Rights Reserved.
            </div>
        </div>
        <!-- * app footer -->

    </div>
    <!-- * App Capsule -->
</div>

    <!-- App Sidebar -->
    <div class="modal fade panelbox panelbox-left" id="sidebarPanel" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-body p-0" style="background-color:#191919; color:#cecece;">
                    <!-- profile box -->
                    <div class="profileBox pt-2 pb-2">
                        <div class="image-wrapper">
                            <img src="/assets/img/sample/avatar/avatar1.jpg" alt="image" class="imaged  w36">
                        </div>
                       
                        <a href="#" class="btn btn-link btn-icon sidebar-close" data-bs-dismiss="modal">
                            <ion-icon name="close-outline"></ion-icon>
                        </a>
                    </div>
                    <!-- * profile box -->

                    <!-- menu -->
                    <div class="listview-title mt-1" style="color:#f2f2f2;">Admin-Menu</div>
                    <ul class="listview flush transparent no-line image-listview">

                        <li>
                            <a href="/adminRoute" class="item">
                                <div class="icon-box bg-primary">
                                    <ion-icon name="pie-chart-outline" style="color:#cecece;"></ion-icon>
                                </div>
                                <div class="in" style="color:#cecece;">
                                    Dashboard
                                </div>
                            </a>
                        </li>

                        <li>
                            <a href="/allFunding" class="item">
                                <div class="icon-box bg-primary">
                                    <ion-icon name="cash-outline" style="color:#cecece;"></ion-icon>
                                </div>
                                <div class="in" style="color:#cecece;">
                                   All  Deposit
                                </div>
                            </a>
                        </li>

                        <li>
                            <a href="/allWidthdrawals" class="item">
                                <div class="icon-box bg-primary">
                                    <ion-icon name="wallet-outline" style="color:#cecece;"></ion-icon>
                                </div>
                                <div class="in" style="color:#cecece;">
                                   All Withdrawals
                                </div>
                            </a>
                        </li>
                                                <li>
                            <a href="/allVerify" class="item">
                                <div class="icon-box bg-primary">
                                    <ion-icon name="document-text-outline" style="color:#cecece;"></ion-icon>
                                </div>
                                <div class="in" style="color:#cecece;">
                                    All Verifications (KYC)
                                </div>
                            </a>
                        </li>
                       
                        <li>
                            <a href="/allinsurance" class="item">
                                <div class="icon-box bg-primary">
                                    <ion-icon name="fitness-outline" style="color:#cecece;"></ion-icon>
                                </div>
                                <div class="in" style="color:#cecece;">
                                   All Insurance 
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="/allsignal" class="item">
                                <div class="icon-box bg-primary">
                                    <ion-icon name="flash-outline" style="color:#cecece;"></ion-icon>
                                </div>
                                <div class="in" style="color:#cecece;">
                                    All Signal
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="/all-accountUpgrade" class="item">
                                <div class="icon-box bg-primary">
                                    <ion-icon name="trending-up-outline" style="color:#cecece;"></ion-icon>
                                </div>
                                <div class="in" style="color:#cecece;">
                                  All  Upgrade
                                </div>
                            </a>
                        </li>
                    </ul>
                    <!-- * menu -->
                </div>
            </div>
        </div>
    </div>
    <!-- * App Sidebar -->



   

    <!-- ========= JS Files =========  -->
    <!-- Bootstrap -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>
    <script src="../plugins/jquery-3.1.1.min.js"></script>
    <script src="../plugins/sweetalerts/sweetalert2.min.js"></script>
    <script src="https://unpkg.com/@popperjs/core@2/dist/umd/popper.js"></script>
    <script src="/assets/js/lib/bootstrap.bundle.min.js"></script>
    <!-- Ionicons -->
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
    <!-- Splide -->
    <script src="/assets/js/plugins/splide/splide.min.js"></script>

</body>

</html>