<div>
    <!-- App Capsule -->
    <div id="appCapsule">
         
         <div class="section mt-2">
            <div class="section-title" style="color:#f2f2f2;">Transaction table</div>
            <p style="color:#cecece;">All Transactions are saved here for refrencing. You can also <code>print</code> Tranactions</p>
            <div class="card" style="background-color:#191919;">

                <div class="table-responsive">
                    <%if(user.widthdraws != ''){%>
                    <table id="datatable" class="table">
                        <thead>
                            <tr>
                                <!-- <th style="color:#f2f2f2;">Type</th> -->
                                                <th style="color:#f2f2f2;">Method</th>
                                                <th style="color:#f2f2f2;">Amount</th>
                                                <!--<th style="color:#f2f2f2;">File</th>-->
                                                <th style="color:#f2f2f2;">Narration</th>
                                                <!-- <th style="color:#f2f2f2;">Time</th> -->
                                                <th style="color:#f2f2f2;">Date</th>
                                                <th style="color:#f2f2f2;">Status</th>
                            </tr>
                        </thead>
                        <tbody>
                                                <% if(user.widthdraws.length > 0 ){ %>
                                             <%  user.widthdraws.forEach(widthdraw => {  %>
                                                                            <tr>
                                                    <!-- <td style="color:#cecece;">Referral Bonus</td> -->
                                                    <td style="color:#cecece;"><%=widthdraw.type%></td>
                                                    <td style="color:#cecece;"> <%=widthdraw.amount%> </td>
                                                    <!--<td style="color:#cecece;">
                                                        Not Available                                                    </td>-->
                                                    <td style="color:#cecece;"><%=widthdraw.narration%></td>
                                                    <td style="color:#cecece;"><%=widthdraw.createdAt%></td>
                                                    <td style="color:#cecece;">
                                                        <b style="color:green;"><i class="las la-exclamation-circle" style="color:green;"></i><%=widthdraw.status%></b>                                                    </td>
                                                </tr>
                                                <%  }) %>
                                             <%  } %>
                                                                </tbody>
                    </table>
                    <button type="button" class="btn btn-text-primary" onclick="window.print()"><i class="las la-download"></i>Tap to Print</button>
                    <% }else{%>
                        <h3 class="text-center text-secondary mt-5">No Widthdrawal found!</h3>
                        <% } %>
                </div>

            </div>
        </div>

    </div>
    <!-- * App Capsule -->
